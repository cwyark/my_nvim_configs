{
  "mcpServers": {
    "git": {
      "disabled": true,
      "args": [
        "mcp-server-git",
        "--repository",
        "${PWD}"
      ],
      "command": "uvx"
    },
    "memory": {
      "disabled": true,
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "command": "npx"
    },
    "sequential-thinking": {
      "custom_instructions": {
        "disabled": true
      },
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx",
      "disabled": true
    },
    "time": {
      "custom_instructions": {
        "disabled": true
      },
      "args": [
        "mcp-server-time",
        "--local-timezone",
        "Asia/Taipei"
      ],
      "command": "uvx",
      "disabled": false
    },
    "puppeteer": {
      "disabled": true,
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "command": "npx"
    },
    "filesystem": {
      "disabled": true,
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "./"
      ],
      "command": "npx"
    },
    "docker": {
      "disabled": false,
      "args": [
        "docker-mcp"
      ],
      "command": "uvx"
    },
    "repomix": {
      "custom_instructions": {
        "disabled": true
      },
      "args": [
        "-y",
        "repomix",
        "--mcp"
      ],
      "command": "npx",
      "disabled": true
    },
    "fetch": {
      "disabled": false,
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    }
  }
}
