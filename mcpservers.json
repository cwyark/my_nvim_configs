{
  "mcpServers": {
    "docker": {
      "args": [
        "docker-mcp"
      ],
      "command": "uvx",
      "disabled": false
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "autoApprove": [],
      "command": "uvx",
      "disabled": false
    },
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "./"
      ],
      "command": "npx",
      "disabled": false
    },
    "git": {
      "args": [
        "mcp-server-git",
        "--repository",
        "${PWD}"
      ],
      "command": "uvx",
      "disabled": false
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "command": "npx",
      "disabled": false
    },
    "sequential-thinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx",
      "custom_instructions": {
        "disabled": true
      },
      "disabled": false
    },
    "time": {
      "args": [
        "mcp-server-time",
        "--local-timezone=Asia/Taipei"
      ],
      "command": "uvx",
      "disabled": false
    }
  },
  "nativeMCPServers": []
}
